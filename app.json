{
  "expo": {
    "name": "quizApp",
    "slug": "quizApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoApp.png",
    "userInterfaceStyle": "light",
    "plugins": ["@config-plugins/react-native-blob-util", "@config-plugins/react-native-pdf"],
    "splash": {
      "image": "./assets/images/LogoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.quizApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoApp.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.quizApp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5e961575-58a6-41be-921e-d97c415d0970"
      }
    }
  }
}
